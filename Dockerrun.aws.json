{
    "AWSEBDockerrunVersion": "2",
    "containerDefinitions": [
        {
            "name": "backend",
            "image": "shamoleambo/todo-backend:latest",
            "memory": 128,
            "portMappings": [
                {
                    "hostPort": 8080,
                    "containerPort": 8080
                }
            ]
        },
        {
            "name": "frontend",
            "image": "shamolemabo/todo-frontend:latest",
            "memory": 128,
            "portMappings": [
                {
                    "hostPort": 3000,
                    "containerPort": 3000
                }
            ]
        }
    ]
}